    <value>-- database unit test for DemoVCom.AdvisorIndexDataGet
DECLARE 
@RC AS INT, 
@DateFrom AS DATETIME, 
@AdvisorMasterEntityID AS INT, 
@CompanyMasterEntityID AS INT, 
@GetOnlyActive AS BIT, 
@GetOnlyInactive AS BIT;
-- DECLARE VALUES TO EXEC STORE PROCEDURE, IT WILL BRING ALL ACTIVE RECORDS FROM 2019-01-02 WITH ALL ADVISORMASTERID AND COMPANYMASTERID
SELECT @RC = 0,
       @DateFrom = N'2019-01-02',
       @AdvisorMasterEntityID = NULL,
       @CompanyMasterEntityID = NULL,
       @GetOnlyActive = 1,
       @GetOnlyInactive = 0;

EXECUTE @RC = [DemoVCom].[AdvisorIndexDataGet] @DateFrom, @AdvisorMasterEntityID, @CompanyMasterEntityID, @GetOnlyActive, @GetOnlyInactive;

SELECT @RC AS RC;

</value>
